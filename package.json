{
  "name": "spoonfeeder",
  "version": "0.0.0",
  "scripts": {
    "upgrade": "npx npm-check-updates --interactive",
    "circular": "dpdm --transform --no-warning --no-tree --exit-code circular:1 src/ts/main.ts",
    "type-check": "vue-tsc --allowJs --noEmit -p tsconfig.typecheck.json --composite false",
    "lint": "eslint src/ts --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --loglevel=warn --write --single-quote \"src/ts/**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test:unit": "vitest --run --environment jsdom --reporter default",
    "test:e2e": "npx cypress run -q",
    "build-only": "vite build"
  },
  "dependencies": {
    "@iconfu/svg-inject": "^1.2.3",
    "@quasar/extras": "^1.17.0",
    "@tanem/svg-injector": "^10.1.68",
    "@types/mathjax": "^0.0.40",
    "asciidoctor": "^3.0.4",
    "axios": "^1.10.0",
    "downdoc": "^1.0.2-stable",
    "dpdm": "^3.14.0",
    "fs": "^0.0.1-security",
    "gsap": "^3.13.0",
    "handlebars": "^4.7.8",
    "highlight.js": "^11.11.1",
    "iframe-resizer": "^5.4.6",
    "jquery": "^3.7.1",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "marked": "^15.0.12",
    "mathjax-full": "^3.2.2",
    "mnemonist": "^0.40.3",
    "mobile-drag-drop": "^3.0.0-rc.0",
    "normalize.css": "^8.0.1",
    "pinia": "^3.0.3",
    "polyfill-library": "^4.8.0",
    "quasar": "^2.11.10",
    "typed.js": "^2.1.0",
    "typescript-eslint": "^0.0.1-alpha.0",
    "vara": "^1.4.1",
    "vue": "^3.5.17",
    "vue-i18n": "^11.1.7",
    "vue-router": "^4.5.1",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^6.0.3",
    "@quasar/app-vite": "^1.2.1",
    "@quasar/quasar-app-extension-qoverlay": "^2.0.1",
    "@rushstack/eslint-patch": "^1.11.0",
    "@types/jquery": "^3.5.32",
    "@types/js-yaml": "^4.0.9",
    "@types/jsdom": "^21.1.7",
    "@types/lodash": "^4.17.18",
    "@types/marked": "^6.0.0",
    "@types/node": "^24.0.3",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "cypress": "^14.5.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "quasar-app-extension-qhierarchy": "^1.0.9",
    "start-server-and-test": "^2.0.0",
    "typescript": "5.0.2",
    "vitest": "^3.2.4",
    "vitest-mock-extended": "^3.1.0",
    "vue-tsc": "^2.2.10"
  },
  "engines": {
    "node": "24",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "packageManager": "yarn@4.9.2"
}
